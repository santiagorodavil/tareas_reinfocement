    "def SARSA(es,gamma,alpha):\n",
    "    # Inicializa s y a de forma eps-greedy e inicia el episodio \n",
    "    done=False\n",
    "    state=1\n",
    "    action,_=eps_greedy(state)\n",
    "    while not done:\n",
    "        # encuentra s'\n",
    "        state_, reward, try_action, real_action, done=es.step(state, action, random=True)\n",
    "        \n",
    "        # Revisa que state_ no sea un estado terminal\n",
    "        if done:\n",
    "            # Valor q(s',a') terminal\n",
    "            Q_=0\n",
    "        else:\n",
    "            # Obtengo a' de s' con epsilon greedy\n",
    "            action_,_=eps_greedy(state_)\n",
    "            Q_=es.Q[state_][action_]\n",
    "        \n",
    "        # Calculo de actualizacion q(s,a) <- q(s,a) + alpha*(R + gamma*q(s',a') - q(s,a)) \n",
    "        es.Q[state][action]+=alpha*(reward+gamma*Q_-es.Q[state][action])\n",
    "        \n",
    "        # asigna a = a' y s = s'\n",
    "        state=state_\n",
    "        action=action_     "
   ]
